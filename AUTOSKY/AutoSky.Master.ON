#!/bin/bash
#
# Turn On Auto SkyWarn
#
#
# SOURCE THIS FILE TO OBTAIN VARIABLE SETTINGS
source /usr/local/AUTOSKY/autoskywarn.conf
#

WX=${BASEDIR}

ENABLEFILE=alert.enbl.master

cd $WX ;

# CREATE ENABLE FILE
$TOUCH $ENABLEFILE

# CONFIRM THE ACTION
if [ "$1" != "quiet" ] ; then
	$TALKIT  Auto Sky\-Warn MASTER Enabled
fi
