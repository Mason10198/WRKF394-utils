#!/bin/bash
#
# Turn On BOBTAILING OF WX-TAIL MESSAGE
#
#
# SOURCE THIS FILE TO OBTAIN VARIABLE SETTINGS
source /usr/local/AUTOSKY/autoskywarn.conf
#

WX=${BASEDIR}

ENABLEFILE=alert.enbl.bobtail

cd $WX ;

# CREATE ENABLE FILE
$TOUCH $ENABLEFILE

# CONFIRM THE ACTION
if [ "$1" != "quiet" ] ; then
	$TALKIT  Auto-SkyWarn Bob-Tail-ing Enabled
fi
